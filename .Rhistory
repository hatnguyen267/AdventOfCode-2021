# Fish farm function
lantern <- function(x){
fishfarm <- x -1
if(0 %in% x){
number_new_fish <- table(x)[1]
# reset 0 fish
reset <- replace(x, x == 0, 7)
# adding new fish
new_fish <- rep(8, times = number_new_fish)
fishfarm <- c(reset, new_fish)
}
fishfarm
}
